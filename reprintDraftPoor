def tablesetup():
    playerInput = input("Where would you like to hit")


    if playerInput:
        rows, cols = (5, 5)
        arr = [["O"] * cols] * rows

        for row in arr:
            print(rows)

# the reprint function so far...
def reprint():
    tablesetup()

reprint()
